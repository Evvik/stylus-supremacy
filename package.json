{
  "name": "stylus-supremacy",
  "description": "Make your Stylus files look great again.",
  "version": "0.2.10",
  "author": {
    "name": "Anantachai Saothong",
    "email": "thisismanta@outlook.com"
  },
  "keywords": [
    "stylus",
    "css",
    "format",
    "pretty",
    "beautify"
  ],
  "bugs": "https://github.com/ThisIsManta/stylus-supremacy/issues",
  "bin": "./edge/command.js",
  "main": "./edge/index.js",
  "scripts": {
    "test": "node ./test/runner.js",
    "docs": "browserify --transform babelify --require ./edge/format.js:format | uglifyjs --compress > docs/format.js && node edge/reviseDocumentation.js",
    "preversion": "npm test",
    "version": "npm publish",
    "postversion": "git push --tags --quiet origin master:master"
  },
  "devDependencies": {
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "jasmine": "^2.5.3",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "glob": "^7.1.1",
    "lodash": "^4.17.4",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5"
  }
}
